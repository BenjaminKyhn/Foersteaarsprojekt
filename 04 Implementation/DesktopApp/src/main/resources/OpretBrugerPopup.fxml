   <children>
      <VBox alignment="CENTER" prefHeight="125.0" prefWidth="250.0" spacing="10.0">
         <children>
            <Label fx:id="txtLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="200.0" text="Infoboks" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Button fx:id="btnOK" alignment="CENTER" mnemonicParsing="false" text="OK">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
