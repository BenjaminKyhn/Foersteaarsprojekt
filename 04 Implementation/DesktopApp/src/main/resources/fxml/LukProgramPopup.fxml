<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="popupAnchorPane" prefHeight="100.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.LukProgramPopupController">
    <children>
        <VBox alignment="CENTER" prefHeight="125.0" prefWidth="250.0" spacing="10.0">
            <children>
                <Label fx:id="lblBesked" alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="200.0" text="Besked" textAlignment="CENTER" wrapText="true">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
            <HBox alignment="CENTER" prefWidth="250.0" spacing="10.0">
               <children>
                      <Button fx:id="btnJa" alignment="CENTER" mnemonicParsing="false" text="Ja" />
                  <Button fx:id="btnNej" mnemonicParsing="false" text="Nej" />
                  <Button fx:id="btnAnnuller" mnemonicParsing="false" text="Annuller" />
               </children>
            </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
