<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="kalenderAnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.KalenderController">
   <children>
      <AnchorPane layoutY="1.0" prefHeight="0.0" prefWidth="800.0">
         <children>
            <MenuBar fx:id="menuBar" prefHeight="26.0" prefWidth="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <menus>
                  <Menu mnemonicParsing="false" text="Menu">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#tilHovedmenu" text="Gå til hovedmenuen" />
                        <MenuItem mnemonicParsing="false" onAction="#logUd" text="Log ud" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <Button fx:id="btnTilbage" mnemonicParsing="false" onAction="#tilHovedmenu" prefWidth="60.0" text="Tilbage" AnchorPane.rightAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="calenderViewHolder" layoutY="27.0" prefHeight="574.0" prefWidth="800.0">
         <children>
            <Label layoutX="90.0" layoutY="241.0" text="Indlæser Kalenderen...">
               <font>
                  <Font size="64.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </children>
</AnchorPane>
